{
  "name": "amabot",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "pnpm": {
    "onlyBuiltDependencies": [
      "bun"
    ]
  },
  "scripts": {
    "check": "run-p check:*",
    "check:biome": "biome ci .",
    "check:types": "tsgo --noEmit",
    "check:cf-typegen": "$npm_execpath run cf-typegen --check",
    "register": "bun dist/register.ts",
    "deploy-dev": "tsdown ./src/index.ts --platform browser --minify && wrangler deploy --name amabot-dev",
    "cf-typegen": "wrangler types"
  },
  "dependencies": {
    "@mathjax/src": "^4.1.0",
    "@resvg/resvg-wasm": "^2.6.2",
    "discord-hono": "^0.20.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@types/node": "25.2.1",
    "@types/service-worker-mock": "^2.0.4",
    "@typescript/native-preview": "7.0.0-dev.20260207.1",
    "bun": "^1.3.8",
    "discord-api-types": "^0.38.38",
    "npm-run-all": "^4.1.5",
    "tsdown": "^0.20.3",
    "wrangler": "^4.63.0"
  }
}
